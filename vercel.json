{
  "version": 2,
  "framework": "vite",
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/api/upload.js"
    },
    {
      "src": "/api/files",
      "dest": "/api/files.js"
    },
    {
      "src": "/api/login",
      "dest": "/api/login.js"
    },
    {
      "src": "/api/search",
      "dest": "/api/search.js"
    },
    {
      "src": "/api/download/(.*)",
      "dest": "/api/download/[...slug].js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}